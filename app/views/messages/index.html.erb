<h1>Message Page</h1>
<ul>
  <%= render partial: "message", collection: @messages %>
</ul>
<hr>
<%= form_for [@room, Message.new] do |f| %>
  <div>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>
  <div>
    <%= f.label :content %>
    <%= f.text_area :content %>
  </div>
  <div>
    <%= f.submit 'Create' %>
  </div>
<% end %>